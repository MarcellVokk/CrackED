<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>CrackED</Title>
    <PackageProjectUrl>https://github.com/Vadkarika2/CrackED</PackageProjectUrl>
    <Authors>Gazdag Marcell</Authors>
    <Description>A performance-oriented text/code editor component for WPF.</Description>
    <PackageTags>text editor,text,editor,code editor,.net,.net core,wpf</PackageTags>
  </PropertyGroup>

</Project>
